{
  "name": "focutron",
  "version": "0.0.1",
  "description": "Google Assistant timers on your desktop",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . Focutron --overwrite --icon=assets/icons/mac/icon.icns --out=build",
