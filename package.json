  "devDependencies": {
    "electron": "4.1.4"
  },
  "dependencies": {
    "cron": "1.7.0",
    "electron-packager": "13.1.1",
    "local-devices": "2.0.0",
    "lodash": "4.17.11",
    "moment": "2.29.2",
    "suncalc": "1.8.0",
    "winston": "3.2.1"
  }
}
